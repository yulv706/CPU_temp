<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="lpm.lpm_components" />
		<HAS_SIM_LIB VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="LPM_NUMWORDS" TYPE="INTEGER" DEFAULT_VALUE="2 exp lpm_widthad" />
		<PARAMETER NAME="LPM_WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_WIDTHAD" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_INDATA" TYPE="STRING" VALUE="REGISTERED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" />
		<PARAMETER NAME="LPM_ADDRESS_CONTROL" TYPE="STRING" VALUE="REGISTERED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="USE_EAB" TYPE="STRING" VALUE="ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="LPM_FILE" TYPE="STRING" VALUE="UNUSED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="LPM_OUTDATA" TYPE="STRING" VALUE="REGISTERED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="lpm_ram_io" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="outenab" TYPE="INPUT" DEFAULT_VALUE="1" SIM_DEFAULT_VALUE="Z" />
		<PORT NAME="inclock" TYPE="INPUT" DEFAULT_VALUE="1" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="outclock" TYPE="INPUT" DEFAULT_VALUE="1" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="address" TYPE="INPUT" WIDTH="LPM_WIDTHAD" DEFAULT_VALUE="0" />
		<PORT NAME="we" TYPE="INPUT" SIM_DEFAULT_VALUE="Z" />
		<PORT NAME="memenab" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="dio" TYPE="BIDIR" WIDTH="LPM_WIDTH" />
	</PORTS>
</ROOT>
