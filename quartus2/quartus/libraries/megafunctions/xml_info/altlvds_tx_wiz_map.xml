<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altlvds_tx_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTLVDS" WIZARD_NAME="ALTLVDS" MODULE_NAME="altlvds_tx" >
			<PARAMETER NAME="Num_Channel" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="NUMBER_OF_CHANNELS" />
			<PARAMETER NAME="Deser_Factor" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="DESERIALIZATION_FACTOR" />
				<PARAMETER NAME="Ext_PLL" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="USE_EXTERNAL_PLL" />						
			<PARAMETER NAME="Le_Serdes" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="IMPLEMENT_IN_LES" />
			<PARAMETER NAME="LVDS_Mode" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="Reg_InOut" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="REGISTERED_INPUT == OFF ? 0 : 1" />
			</PARAMETER>			
			<PARAMETER NAME="Clock_Mode" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="1" />
			<PARAMETER NAME="Use_Lock" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(tx_locked) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="PLL_Enable" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(tx_pll_enable) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="PLL_Freq" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(1000000 double_div INCLOCK_PERIOD)" />
			</PARAMETER>
			<PARAMETER NAME="PLL_Period" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="INCLOCK_PERIOD double_div 1000" />
			</PARAMETER>
			<PARAMETER NAME="Int_Device" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INTENDED_DEVICE_FAMILY" />			
			<PARAMETER NAME="Inclock_Boost" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="INCLOCK_BOOST" />
			<PARAMETER NAME="CDS_Mode" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="CDS_MODE == MULTIPLE_BIT ? 2 : (CDS_MODE == SINGLE_BIT ? 1 : 0)" />
			</PARAMETER>
			<PARAMETER NAME="Multi_Clock" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(MULTI_CLOCK == ON) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Use_Common_Rx_Tx_Plls" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(COMMON_RX_TX_PLL == ON) ? 1 : 0" />
			</PARAMETER>	
			<PARAMETER NAME="Data_rate" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="OUTPUT_DATA_RATE" />
			<PARAMETER NAME="Input_Clock_alignment" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INCLOCK_DATA_ALIGNMENT" />
			<PARAMETER NAME="INCLOCK_PHASE_SHIFT" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(INCLOCK_PHASE_SHIFT) ? INCLOCK_PHASE_SHIFT : UNUSED" />
			</PARAMETER>
			<PARAMETER NAME="MSB_Cente_Align" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(CENTER_ALIGN_MSB == ON) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Clock_Choices" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="REGISTERED_INPUT" />
			<PARAMETER NAME="Use_CoreClock" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(tx_coreclock) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Use_Clock_Resc" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="OUTCLOCK_RESOURCE" />
			<PARAMETER NAME="Outclock_Divide_By" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="OUTCLOCK_DIVIDE_BY" />
			<PARAMETER NAME="OUTCLOCK_MULTIPLY_BY" TYPE="INTEGER" CNX_TYPE="CONSTANT" VALUE="OUTCLOCK_MULTIPLY_BY" />
			<PARAMETER NAME="CORECLOCK_DIVIDE_BY" TYPE="INTEGER" CNX_TYPE="CONSTANT" VALUE="CORECLOCK_DIVIDE_BY" />
			<PARAMETER NAME="Output_Clock_Alignment" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="OUTCLOCK_ALIGNMENT" />
			<PARAMETER NAME="OUTCLOCK_PHASE_SHIFT" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(OUTCLOCK_PHASE_SHIFT) ? OUTCLOCK_PHASE_SHIFT : UNUSED" />
			</PARAMETER>
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
