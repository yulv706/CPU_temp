<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="lpm.lpm_components" />
		<HAS_SIM_LIB VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="CARRY_CHAIN" TYPE="STRING" VALUE="MANUAL|AUTO|IGNORE" DEFAULT_VALUE_INDEX="0" LPM_HINT="ON" />
		<PARAMETER NAME="CARRY_CHAIN_LENGTH" TYPE="INTEGER" DEFAULT_VALUE="32" LPM_HINT="ON" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="LPM_DIRECTION" TYPE="STRING" VALUE="ADD|SUB|DEFAULT|UNUSED" DEFAULT_VALUE_INDEX="2" />
		<PARAMETER NAME="LPM_PIPELINE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="LPM_REPRESENTATION" TYPE="STRING" VALUE="SIGNED|UNSIGNED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="LPM_WIDTH" TYPE="INTEGER" />
