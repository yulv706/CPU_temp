	</PARAMETERS>
	<PORTS>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="add_sub" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="cin" TYPE="INPUT" DEFAULT_VALUE="Z" />
		<PORT NAME="clken" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="clock" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="cout" TYPE="OUTPUT" />
		<PORT NAME="dataa" TYPE="INPUT" WIDTH="LPM_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="datab" TYPE="INPUT" WIDTH="LPM_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="overflow" TYPE="OUTPUT" />
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="LPM_WIDTH" />
	</PORTS>
</ROOT>
