<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altshift_taps_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTSHIFT_TAPS" WIZARD_NAME="ALTSHIFT_TAPS" MODULE_NAME="altshift_taps" >
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="INTENDED_DEVICE_FAMILY" />
			<PORT NAME="clken" CNX_TYPE="USED_PORT" />
			<PORT NAME="aclr" CNX_TYPE="USED_PORT" />			
			<PARAMETER NAME="WIDTH" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH" />
			<PARAMETER NAME="NUMBER_OF_TAPS" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="NUMBER_OF_TAPS" />
			<PARAMETER NAME="TAP_DISTANCE" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="TAP_DISTANCE" />
			<PARAMETER NAME="GROUP_TAPS" CNX_TYPE="PRIVATE" TYPE="INTEGER" VALUE="0" />
			<PARAMETER NAME="RAM_BLOCK_TYPE" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="(m_mram || m_m144k) ? 2 : ((m_m4k || m_m8k) ? 1 : ((m_m512 || m_lutram) ? 1 : 3))"/>
			</PARAMETER>
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
