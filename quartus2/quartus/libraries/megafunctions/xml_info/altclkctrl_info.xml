<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="CLOCK_TYPE" TYPE="STRING" VALUE="AUTO|GCLK|LCLK|EXTCLK|SIDE_CLK|Global Clock|Regional Clock|External Clock Output|Dual-Regional Clock|Periphery clock" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="ENA_REGISTER_MODE" TYPE="STRING" VALUE="none|falling edge|double register" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="IMPLEMENT_IN_LES" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="NUMBER_OF_CLOCKS" TYPE="INTEGER" DEFAULT_VALUE="4" />
		<PARAMETER NAME="USE_GLITCH_FREE_SWITCH_OVER_IMPLEMENTATION" TYPE="STRING" VALUE="ON|OFF" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="WIDTH_CLKSELECT" TYPE="INTEGER" DEFAULT_VALUE="2" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altclkctrl" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="clkselect" TYPE="INPUT" WIDTH="WIDTH_CLKSELECT" DEFAULT_VALUE="0" />
		<PORT NAME="ena" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="inclk" TYPE="INPUT" WIDTH="NUMBER_OF_CLOCKS" DEFAULT_VALUE="0" />
		<PORT NAME="outclk" TYPE="OUTPUT" />
	</PORTS>
</ROOT>
