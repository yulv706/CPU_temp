<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="mfam_family_macros.xml" />
	<INCLUDE FILE="scfifo_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="FIFO" WIZARD_NAME="FIFO" MODULE_NAME="scfifo" >
			<PARAMETER NAME="Width" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="LPM_WIDTH" />
			<PARAMETER NAME="Depth" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="LPM_NUMWORDS" />
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="Clock" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="Full" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(full) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Empty" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(empty) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="UsedW" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(usedw) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="AlmostFull" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(almost_full) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="AlmostEmpty" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(almost_empty) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="sc_aclr" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(aclr) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="sc_sclr" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(sclr) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="AlmostFullThr" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="ALMOST_FULL_VALUE" />
			<PARAMETER NAME="AlmostEmptyThr" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="ALMOST_EMPTY_VALUE" />
			<PARAMETER NAME="rsFull" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="rsEmpty" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="rsUsedW" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="wsFull" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="wsEmpty" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="wsUsedW" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="dc_aclr" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="OVERFLOW_CHECKING" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="OVERFLOW_CHECKING == ON ? 0 : 1" />
			</PARAMETER>
			<PARAMETER NAME="UNDERFLOW_CHECKING" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="UNDERFLOW_CHECKING == ON ? 0 : 1" />
			</PARAMETER>
			<PARAMETER NAME="LE_BasedFIFO" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="USE_EAB == OFF ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="LegacyRREQ" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="LPM_SHOWAHEAD == OFF ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="CLOCKS_ARE_SYNCHRONIZED" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="RAM_BLOCK_TYPE" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="0" />
			<PARAMETER NAME="Optimize" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="ADD_RAM_OUTPUT_REGISTER == ON ? 1 : 2" />
			</PARAMETER>
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
