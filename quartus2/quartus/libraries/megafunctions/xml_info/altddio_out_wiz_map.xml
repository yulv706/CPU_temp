<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altddio_out_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTDDIO_OUT" WIZARD_NAME="ALTDDIO_OUT" MODULE_NAME="altddio_out" >
			<PARAMETER NAME="WIDTH" CNX_TYPE="PRIVATE" TYPE="INTEGER" VALUE="WIDTH" />
			<PARAMETER NAME="ARESET_MODE" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="is_used(aclr) ? 0 : (is_used(aset) ? 1 : 2)"/>
			</PARAMETER>
			<PARAMETER NAME="POWER_UP_HIGH" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="POWER_UP_HIGH == ON ? 1 : 0"/>
			</PARAMETER>
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="CLKEN" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="is_used(outclocken) ? 1 : 0"/>
			</PARAMETER>
			<PARAMETER NAME="OE" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="is_used(oe) ? 1 : 0"/>
			</PARAMETER>
			<PARAMETER NAME="OE_REG" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="OE_REG == REGISTERED ? 1 : 0"/>
			</PARAMETER>
			<PARAMETER NAME="EXTEND_OE_DISABLE" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="EXTEND_OE_DISABLE == ON ? 1 : 0"/>
			</PARAMETER>
			<PARAMETER NAME="USE_COMBOUT" CNX_TYPE="PRIVATE" TYPE="INTEGER" VALUE="0" />
			<PARAMETER NAME="USE_DATAOUT" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="1" />
			<PARAMETER NAME="INVERT_OUTPUT" CNX_TYPE="CONSTANT" TYPE="STRING" VALUE="INVERT_OUTPUT" />			
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
