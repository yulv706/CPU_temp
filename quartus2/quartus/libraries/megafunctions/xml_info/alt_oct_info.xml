<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="altera_mf.altera_mf_components" />
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="ALLOW_SERIAL_DATA_FROM_CORE" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="ENABLE_PARALLEL_TERMINATION" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="OCT_BLOCK_NUMBER" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="POWER_DOWN" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="TEST_MODE" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="alt_oct" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="cal_shift_busy" TYPE="OUTPUT" WIDTH="OCT_BLOCK_NUMBER" />
		<PORT NAME="calibration_busy" TYPE="OUTPUT" WIDTH="OCT_BLOCK_NUMBER" />
		<PORT NAME="calibration_done" TYPE="OUTPUT" WIDTH="OCT_BLOCK_NUMBER" />
		<PORT NAME="calibration_request" TYPE="INPUT" WIDTH="OCT_BLOCK_NUMBER" DEFAULT_VALUE="0" />
		<PORT NAME="calibration_wait" TYPE="INPUT" WIDTH="OCT_BLOCK_NUMBER" DEFAULT_VALUE="0" />
		<PORT NAME="clken" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="clock" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="incrdn" TYPE="OUTPUT" />
		<PORT NAME="incrup" TYPE="OUTPUT" />
		<PORT NAME="parallelterminationcontrol" TYPE="OUTPUT" WIDTH="OCT_BLOCK_NUMBER * 14" />
		<PORT NAME="rdn" TYPE="INPUT" WIDTH="OCT_BLOCK_NUMBER" />
		<PORT NAME="rup" TYPE="INPUT" WIDTH="OCT_BLOCK_NUMBER" />
		<PORT NAME="s2pload" TYPE="INPUT" WIDTH="OCT_BLOCK_NUMBER" DEFAULT_VALUE="0" />
		<PORT NAME="scanout" TYPE="OUTPUT" />
		<PORT NAME="seriesterminationcontrol" TYPE="OUTPUT" WIDTH="OCT_BLOCK_NUMBER * 14" />
		<PORT NAME="shiftregisterprobe" TYPE="OUTPUT" />
		<PORT NAME="termination_control" TYPE="OUTPUT" WIDTH="16 * OCT_BLOCK_NUMBER" />
		<PORT NAME="terminationcontrolprobe" TYPE="OUTPUT" />
	</PORTS>
</ROOT>
