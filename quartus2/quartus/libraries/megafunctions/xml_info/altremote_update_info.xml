<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="IN_DATA_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="12" />
		<PARAMETER NAME="OPERATION_MODE" TYPE="STRING" VALUE="remote|local|active_serial_remote" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="OUT_DATA_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="12" />
		<PARAMETER NAME="SIM_INIT_CONFIG" TYPE="STRING" VALUE="factory|application" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="SIM_INIT_PAGE_SELECT" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="SIM_INIT_STATUS" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="SIM_INIT_WATCHDOG_VALUE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altremote_update" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="busy" TYPE="OUTPUT" />
		<PORT NAME="clock" TYPE="INPUT" />
		<PORT NAME="data_in" TYPE="INPUT" WIDTH="in_data_width" DEFAULT_VALUE="0" />
		<PORT NAME="data_out" TYPE="OUTPUT" WIDTH="out_data_width" />
		<PORT NAME="param" TYPE="INPUT" WIDTH="3" DEFAULT_VALUE="0" />
		<PORT NAME="pgmout" TYPE="OUTPUT" WIDTH="3" />
		<PORT NAME="read_param" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="read_source" TYPE="INPUT" WIDTH="2" DEFAULT_VALUE="0" />
		<PORT NAME="reconfig" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="reset" TYPE="INPUT" />
		<PORT NAME="reset_timer" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="write_param" TYPE="INPUT" DEFAULT_VALUE="0" />
	</PORTS>
</ROOT>
