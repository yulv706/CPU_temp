<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="mfam_family_macros.xml" />

	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="altera_mf.altera_mf_components"/>
	</FUNCTION_INFORMATION>

	<PARAMETER_RULES>
		<PARAMETER NAME="WIDTH_DATAWORD">
			<FORCE_RANGE MAX="64" MIN="1"/>
		</PARAMETER>
		<PARAMETER NAME="WIDTH_CODEWORD">
			<FORCE_VALUE TO="4" WHEN="WIDTH_DATAWORD == 1"/>
			<FORCE_VALUE TO="WIDTH_DATAWORD + 4" WHEN="(WIDTH_DATAWORD gt 1) and (WIDTH_DATAWORD lt 5)"/>
			<FORCE_VALUE TO="WIDTH_DATAWORD + 5" WHEN="(WIDTH_DATAWORD gt 4) and (WIDTH_DATAWORD lt 12)"/>
			<FORCE_VALUE TO="WIDTH_DATAWORD + 6" WHEN="(WIDTH_DATAWORD gt 11) and (WIDTH_DATAWORD lt 27)"/>
			<FORCE_VALUE TO="WIDTH_DATAWORD + 7" WHEN="(WIDTH_DATAWORD gt 26) and (WIDTH_DATAWORD lt 58)"/>
			<FORCE_VALUE TO="WIDTH_DATAWORD + 8" WHEN="(WIDTH_DATAWORD gt 57) and (WIDTH_DATAWORD lt 73)"/>
		</PARAMETER>
		<PARAMETER NAME="LPM_PIPELINE">
			<FORCE_RANGE MAX="3" MIN="0"/>
		</PARAMETER>
	</PARAMETER_RULES>

	<PORT_RULES>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" USAGE="VARIABLE" >
			<STATE V="UNUSED" WHEN="lpm_pipeline == 0"/>
		</PORT>
		<PORT NAME="clock" TYPE="INPUT" DEFAULT_VALUE="0" IS_CLOCK_PORT="TRUE" USAGE="VARIABLE" >
			<STATE V="USED" WHEN="lpm_pipeline > 0" ELSE="UNUSED"/>
		</PORT>
		<PORT NAME="clocken" TYPE="INPUT" DEFAULT_VALUE="1" USAGE="VARIABLE" >
			<STATE V="UNUSED" WHEN="lpm_pipeline == 0"/>
		</PORT>
	</PORT_RULES>

</ROOT>