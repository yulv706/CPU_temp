<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="CALIBRATE_FOR_ALL_CHANNELS" TYPE="STRING" VALUE="ON|OFF" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="CALIBRATION_START_THRESHOLD" TYPE="INTEGER" DEFAULT_VALUE="256" />
		<PARAMETER NAME="CALIBRATION_WAIT_TIMER" TYPE="INTEGER" DEFAULT_VALUE="1024" />
		<PARAMETER NAME="NUMBER_OF_CHANNELS" TYPE="INTEGER" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" LPM_HINT="ON" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altcal_dpa_pll" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="calibration_busy" TYPE="OUTPUT" />
		<PORT NAME="clock" TYPE="INPUT" />
		<PORT NAME="dpa_fifo_reset" TYPE="OUTPUT" WIDTH="NUMBER_OF_CHANNELS" />
		<PORT NAME="dpa_lock_out" TYPE="OUTPUT" WIDTH="NUMBER_OF_CHANNELS" />
		<PORT NAME="dpa_lock_reset" TYPE="OUTPUT" WIDTH="NUMBER_OF_CHANNELS" />
		<PORT NAME="dpa_locked" TYPE="INPUT" WIDTH="NUMBER_OF_CHANNELS" />
		<PORT NAME="dpa_reset" TYPE="INPUT" WIDTH="NUMBER_OF_CHANNELS" DEFAULT_VALUE="0" />
		<PORT NAME="pll_locked" TYPE="INPUT" />
		<PORT NAME="pll_phasecounterselect" TYPE="OUTPUT" WIDTH="4" />
		<PORT NAME="pll_phasedone" TYPE="INPUT" />
		<PORT NAME="pll_phasestep" TYPE="OUTPUT" />
		<PORT NAME="pll_phaseupdown" TYPE="OUTPUT" />
		<PORT NAME="pll_scanclk" TYPE="INPUT" />
		<PORT NAME="recalibrate" TYPE="INPUT" DEFAULT_VALUE="0" />
	</PORTS>
</ROOT>
