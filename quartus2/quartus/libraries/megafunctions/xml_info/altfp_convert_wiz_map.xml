<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altfp_convert_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTFP_CONVERT" WIZARD_NAME="ALTFP_CONVERT" MODULE_NAME="altfp_convert" >
			<PARAMETER NAME="FPM_FORMAT" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="((WIDTH_EXP_INPUT == 8) and (WIDTH_MAN_INPUT == 23)) ? 0 : (((WIDTH_EXP_INPUT == 11) and (WIDTH_MAN_INPUT == 52)) ? 1 : 2)"/>
				<FORCE_VALUE TO="((WIDTH_EXP_OUTPUT == 8) and (WIDTH_MAN_OUTPUT == 23)) ? 0 : (((WIDTH_EXP_OUTPUT == 11) and (WIDTH_MAN_OUTPUT == 52)) ? 1 : 2)"/>
				<FORCE_VALUE TO="(WIDTH_DATA == 32) ? 0 : ((WIDTH_DATA == 64) ? 1 : 2)"/>
				<FORCE_VALUE TO="(WIDTH_RESULT == 32) ? 0 : ((WIDTH_RESULT == 64) ? 1 : 2)"/>				
			</PARAMETER>
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="WIDTH_EXP_INPUT" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH_EXP_INPUT" />
			<PARAMETER NAME="WIDTH_MAN_INPUT" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH_MAN_INPUT" />
			<PARAMETER NAME="WIDTH_DATA" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH_DATA" />
			<PARAMETER NAME="WIDTH_EXP_OUTPUT" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH_EXP_OUTPUT" />
			<PARAMETER NAME="WIDTH_MAN_OUTPUT" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH_MAN_OUTPUT" />
			<PARAMETER NAME="WIDTH_RESULT" CNX_TYPE="CONSTANT" TYPE="INTEGER" VALUE="WIDTH_RESULT" />
			<PARAMETER NAME="OPERATION" CNX_TYPE="CONSTANT" TYPE="STRING" VALUE="OPERATION" />
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
