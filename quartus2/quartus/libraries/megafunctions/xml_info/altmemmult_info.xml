<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="COEFF_REPRESENTATION" TYPE="STRING" VALUE="SIGNED|UNSIGNED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="COEFFICIENT0" TYPE="STRING" VALUE="UNUSED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="DATA_REPRESENTATION" TYPE="STRING" VALUE="SIGNED|UNSIGNED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="MAX_CLOCK_CYCLES_PER_RESULT" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="NUMBER_OF_COEFFICIENTS" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="RAM_BLOCK_TYPE" TYPE="STRING" VALUE="AUTO|SMALL|MEDIUM|M9K|M4K|M512|MLAB(LUTRAM)" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="TOTAL_LATENCY" TYPE="INTEGER" />
		<PARAMETER NAME="WIDTH_C" TYPE="INTEGER" />
		<PARAMETER NAME="WIDTH_D" TYPE="INTEGER" />
		<PARAMETER NAME="WIDTH_R" TYPE="INTEGER" />
		<PARAMETER NAME="WIDTH_S" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altmemmult" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="clock" TYPE="INPUT" />
		<PORT NAME="coeff_in" TYPE="INPUT" WIDTH="WIDTH_C" DEFAULT_VALUE="0" />
		<PORT NAME="data_in" TYPE="INPUT" WIDTH="WIDTH_D" />
		<PORT NAME="load_done" TYPE="OUTPUT" />
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="WIDTH_R" />
		<PORT NAME="result_valid" TYPE="OUTPUT" />
		<PORT NAME="sclr" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="sel" TYPE="INPUT" WIDTH="WIDTH_S" DEFAULT_VALUE="0" />
		<PORT NAME="sload_coeff" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="sload_data" TYPE="INPUT" DEFAULT_VALUE="0" />
	</PORTS>
</ROOT>
