<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="AEQ_MODE" TYPE="STRING" VALUE="LOCK|RUN" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="AEQ_TRANSLATE_EQS" TYPE="STRING" VALUE="YES|NO" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="BASE_PORT_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="CHANNEL_ADDRESS_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="ENABLE_AEQ" TYPE="STRING" VALUE="ON|OFF" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_BUF_CAL" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_BUF_CAL_FUNC_SIM" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_CHL_ADDR_FOR_ANALOG_CTRL" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_ILLEGAL_MODE_CHECK" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_RX_TX_DUPLEX_SEL" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_SELF_RECOVERY" TYPE="STRING" VALUE="TRUE|FALSE" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="LOGICAL_PLL_SEL_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="MIF_ADDRESS_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="5" />
		<PARAMETER NAME="NUMBER_OF_CHANNELS" TYPE="INTEGER" />
		<PARAMETER NAME="NUMBER_OF_RECONFIG_PORTS" TYPE="INTEGER" />
		<PARAMETER NAME="READ_BASE_PORT_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="RECONFIG_FROMGXB_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="RECONFIG_MODE_SEL_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="3" />
		<PARAMETER NAME="RECONFIG_TOGXB_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="3" />
		<PARAMETER NAME="RX_EQDCGAIN_PORT_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="2" />
		<PARAMETER NAME="TX_PREEMP_PORT_WIDTH" TYPE="INTEGER" DEFAULT_VALUE="4" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="alt2gxb_reconfig" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="aeq_fromgxb" TYPE="INPUT" WIDTH="NUMBER_OF_CHANNELS*6" DEFAULT_VALUE="0" />
		<PORT NAME="aeq_togxb" TYPE="OUTPUT" WIDTH="NUMBER_OF_CHANNELS*4" />
		<PORT NAME="busy" TYPE="OUTPUT" />
		<PORT NAME="channel_reconfig_done" TYPE="OUTPUT" />
		<PORT NAME="data_valid" TYPE="OUTPUT" />
		<PORT NAME="error" TYPE="OUTPUT" />
		<PORT NAME="gxb_address" TYPE="INPUT" WIDTH="3" DEFAULT_VALUE="0" />
		<PORT NAME="logical_channel_address" TYPE="INPUT" WIDTH="CHANNEL_ADDRESS_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="logical_tx_pll_sel" TYPE="INPUT" WIDTH="LOGICAL_PLL_SEL_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="logical_tx_pll_sel_en" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="rate_switch_ctrl" TYPE="INPUT" WIDTH="2" DEFAULT_VALUE="0" />
		<PORT NAME="rate_switch_out" TYPE="OUTPUT" WIDTH="2" />
		<PORT NAME="read" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="reconfig_address" TYPE="INPUT" WIDTH="MIF_ADDRESS_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="reconfig_address_en" TYPE="OUTPUT" />
		<PORT NAME="reconfig_address_out" TYPE="OUTPUT" WIDTH="MIF_ADDRESS_WIDTH" />
		<PORT NAME="reconfig_clk" TYPE="INPUT" />
		<PORT NAME="reconfig_data" TYPE="INPUT" WIDTH="16" DEFAULT_VALUE="0" />
		<PORT NAME="reconfig_data_mask" TYPE="INPUT" WIDTH="16" DEFAULT_VALUE="0" />
		<PORT NAME="reconfig_data_out" TYPE="OUTPUT" WIDTH="16" />
		<PORT NAME="reconfig_fromgxb" TYPE="INPUT" WIDTH="RECONFIG_FROMGXB_WIDTH" />
		<PORT NAME="reconfig_mode_sel" TYPE="INPUT" WIDTH="RECONFIG_MODE_SEL_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="reconfig_togxb" TYPE="OUTPUT" WIDTH="RECONFIG_TOGXB_WIDTH" />
		<PORT NAME="reset_reconfig_address" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="rx_eqctrl" TYPE="INPUT" WIDTH="BASE_PORT_WIDTH*4" DEFAULT_VALUE="0" />
		<PORT NAME="rx_eqctrl_out" TYPE="OUTPUT" WIDTH="READ_BASE_PORT_WIDTH*4" />
		<PORT NAME="rx_eqdcgain" TYPE="INPUT" WIDTH="BASE_PORT_WIDTH*RX_EQDCGAIN_PORT_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="rx_eqdcgain_out" TYPE="OUTPUT" WIDTH="READ_BASE_PORT_WIDTH*RX_EQDCGAIN_PORT_WIDTH" />
		<PORT NAME="rx_tx_duplex_sel" TYPE="INPUT" WIDTH="2" DEFAULT_VALUE="0" />
		<PORT NAME="tx_preemp_0t" TYPE="INPUT" WIDTH="BASE_PORT_WIDTH*TX_PREEMP_PORT_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="tx_preemp_0t_out" TYPE="OUTPUT" WIDTH="READ_BASE_PORT_WIDTH*TX_PREEMP_PORT_WIDTH" />
		<PORT NAME="tx_preemp_1t" TYPE="INPUT" WIDTH="BASE_PORT_WIDTH*TX_PREEMP_PORT_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="tx_preemp_1t_out" TYPE="OUTPUT" WIDTH="READ_BASE_PORT_WIDTH*TX_PREEMP_PORT_WIDTH" />
		<PORT NAME="tx_preemp_2t" TYPE="INPUT" WIDTH="BASE_PORT_WIDTH*TX_PREEMP_PORT_WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="tx_preemp_2t_out" TYPE="OUTPUT" WIDTH="READ_BASE_PORT_WIDTH*TX_PREEMP_PORT_WIDTH" />
		<PORT NAME="tx_vodctrl" TYPE="INPUT" WIDTH="BASE_PORT_WIDTH*3" DEFAULT_VALUE="0" />
		<PORT NAME="tx_vodctrl_out" TYPE="OUTPUT" WIDTH="READ_BASE_PORT_WIDTH*3" />
		<PORT NAME="write_all" TYPE="INPUT" DEFAULT_VALUE="0" />
	</PORTS>
</ROOT>
