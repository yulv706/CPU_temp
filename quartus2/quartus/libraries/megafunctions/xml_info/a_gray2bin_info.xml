<?xml version="1.0"?>
<ROOT>
	<PARAMETERS>
		<PARAMETER NAME="CARRY_CHAIN" TYPE="STRING" VALUE="MANUAL|IGNORE" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="CARRY_CHAIN_LENGTH" TYPE="INTEGER" DEFAULT_VALUE="32" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="IGNORE_CARRY_BUFFERS" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="a_gray2bin" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="bin" TYPE="OUTPUT" WIDTH="width" />
		<PORT NAME="gray" TYPE="INPUT" WIDTH="width" />
	</PORTS>
</ROOT>
