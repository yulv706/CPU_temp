<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="mfam_family_macros.xml" />
	<PARAMETERS>
		<PARAMETER NAME="MSW_SUBTRACT" TYPE="STRING" DEFAULT_VALUE="NO">
			<FORCE_RANGE TO="NO" WHEN="REPRESENTATION == UNSIGNED"/>
		</PARAMETER>
		<PARAMETER NAME="PIPELINE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="REPRESENTATION" TYPE="STRING" DEFAULT_VALUE="UNSIGNED">
				<FORCE_RANGE TO="SIGNED|UNSIGNED" />
		</PARAMETER>
		<PARAMETER NAME="RESULT_ALIGNMENT" TYPE="STRING" DEFAULT_VALUE="LSB">
				<FORCE_RANGE TO="LSB|MSB" />
		</PARAMETER>
		<PARAMETER NAME="SHIFT" TYPE="INTEGER" DEFAULT_VALUE="0">
			<FORCE_RANGE MAX="WIDTH - 1" MIN="0" />
		</PARAMETER>
		<PARAMETER NAME="SIZE" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="WIDTH" TYPE="INTEGER" DEFAULT_VALUE="4" />
		<PARAMETER NAME="WIDTHR" TYPE="INTEGER" DEFAULT_VALUE="4" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" DEFAULT_VALUE="stratixiii" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="clock" TYPE="INPUT" DEFAULT_VALUE="0" IS_CLOCK_PORT="TRUE" USAGE="VARIABLE">
			<STATE V="UNUSED" WHEN="PIPELINE == 0"/>
			<STATE V="USED" WHEN="PIPELINE gt 0" />
		</PORT>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" USAGE="VARIABLE">
			<STATE V="UNUSED" WHEN="PIPELINE == 0"/>
		</PORT>
		<PORT NAME="clken" TYPE="INPUT" HIGH_PROBABILITY="90" USAGE="VARIABLE">
			<STATE V="UNUSED" WHEN="PIPELINE == 0"/>
		</PORT>
		<PORT NAME="data" TYPE="INPUT" WIDTH="SIZE*WIDTH" USAGE="USED"/>
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="WIDTHR" USAGE="USED" />
	</PORTS>
</ROOT>
