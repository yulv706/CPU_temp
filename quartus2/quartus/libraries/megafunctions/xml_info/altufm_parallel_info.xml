<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="ACCESS_MODE" TYPE="STRING" VALUE="READ_WRITE|READ_ONLY" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="ERASE_TIME" TYPE="INTEGER" DEFAULT_VALUE="500000000" />
		<PARAMETER NAME="LPM_FILE" TYPE="STRING" VALUE="UNUSED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="OSC_FREQUENCY" TYPE="INTEGER" DEFAULT_VALUE="180000" />
		<PARAMETER NAME="PROGRAM_TIME" TYPE="INTEGER" DEFAULT_VALUE="1600000" />
		<PARAMETER NAME="WIDTH_ADDRESS" TYPE="INTEGER" DEFAULT_VALUE="9" />
		<PARAMETER NAME="WIDTH_DATA" TYPE="INTEGER" DEFAULT_VALUE="16" />
		<PARAMETER NAME="WIDTH_UFM_ADDRESS" TYPE="INTEGER" DEFAULT_VALUE="9" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altufm_parallel" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="addr" TYPE="INPUT" WIDTH="WIDTH_ADDRESS" />
		<PORT NAME="data_valid" TYPE="OUTPUT" />
		<PORT NAME="datain" TYPE="INPUT" WIDTH="WIDTH_DATA" DEFAULT_VALUE="0" />
		<PORT NAME="dataout" TYPE="OUTPUT" WIDTH="WIDTH_DATA" />
		<PORT NAME="nbusy" TYPE="OUTPUT" />
		<PORT NAME="nerase" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="nread" TYPE="INPUT" />
		<PORT NAME="nwrite" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="osc" TYPE="OUTPUT" />
		<PORT NAME="oscena" TYPE="INPUT" DEFAULT_VALUE="1" />
	</PORTS>
</ROOT>
