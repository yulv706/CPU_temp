<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altremote_update_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTREMOTE_UPDATE" WIZARD_NAME="ALTREMOTE_UPDATE" MODULE_NAME="altremote_update" >
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="SIM_INIT_CONFIG_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="SIM_INIT_CONFIG" />
<!-- type refers to the private type or the constant type? -->
			<PARAMETER NAME="SIM_INIT_PAGE_SELECT_COMBO" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="SIM_INIT_PAGE_SELECT" />
			<PARAMETER NAME="SIM_INIT_STAT_BIT0_CHECK" CNX_TYPE="PRIVATE" TYPE="STRING">
				<FORCE_VALUE TO="(SIM_INIT_STATUS % 2)? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="SIM_INIT_STAT_BIT1_CHECK" CNX_TYPE="PRIVATE" TYPE="STRING">
				<FORCE_VALUE TO="((SIM_INIT_STATUS/2) % 2)? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="SIM_INIT_STAT_BIT2_CHECK" CNX_TYPE="PRIVATE" TYPE="STRING">
				<FORCE_VALUE TO="((SIM_INIT_STATUS/4) % 2)? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="SIM_INIT_STAT_BIT3_CHECK" CNX_TYPE="PRIVATE" TYPE="STRING">
				<FORCE_VALUE TO="((SIM_INIT_STATUS/8) % 2)? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="SIM_INIT_STAT_BIT4_CHECK" CNX_TYPE="PRIVATE" TYPE="STRING">
				<FORCE_VALUE TO="((SIM_INIT_STATUS/16) % 2)? 1 : 0" />
			</PARAMETER>
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
