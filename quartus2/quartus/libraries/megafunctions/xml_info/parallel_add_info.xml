		<PARAMETER NAME="WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="WIDTHR" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
