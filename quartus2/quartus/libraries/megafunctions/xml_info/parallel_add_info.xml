		<PORT NAME="data" TYPE="INPUT" WIDTH="SIZE*WIDTH" DEFAULT_VALUE="0" />
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="WIDTHR" />
	</PORTS>
