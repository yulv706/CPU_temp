		<PORT NAME="outclock" TYPE="INPUT" DEFAULT_VALUE="1" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="q" TYPE="OUTPUT" WIDTH="LPM_WIDTH" />
		<PORT NAME="we" TYPE="INPUT" DEFAULT_VALUE="1" />
	</PORTS>
</ROOT>
