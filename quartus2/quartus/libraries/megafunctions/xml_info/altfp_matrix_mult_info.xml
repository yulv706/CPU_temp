<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="BLOCKS" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="CLUSTER" TYPE="INTEGER" DEFAULT_VALUE="8" />
		<PARAMETER NAME="COLUMNSAA" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="COLUMNSBB" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="ROWSAA" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="VECTORSIZE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="WIDTH_EXP" TYPE="INTEGER" DEFAULT_VALUE="8" />
		<PARAMETER NAME="WIDTH_MAN" TYPE="INTEGER" DEFAULT_VALUE="23" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altfp_matrix_mult" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="calcmatrix" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="done" TYPE="OUTPUT" />
		<PORT NAME="enable" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="loadaa" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="loadbb" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="loaddata" TYPE="INPUT" WIDTH="WIDTH_EXP+WIDTH_MAN+1" DEFAULT_VALUE="0" />
		<PORT NAME="outdata" TYPE="OUTPUT" WIDTH="WIDTH_EXP+WIDTH_MAN+1" />
		<PORT NAME="outvalid" TYPE="OUTPUT" />
		<PORT NAME="ready" TYPE="OUTPUT" />
		<PORT NAME="reset" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="sysclk" TYPE="INPUT" />
	</PORTS>
</ROOT>
