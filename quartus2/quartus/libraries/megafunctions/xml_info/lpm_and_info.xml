<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="lpm.lpm_components" />
		<HAS_SIM_LIB VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="LPM_WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_SIZE" TYPE="INTEGER" />
		<PARAMETER NAME="CARRY_CHAIN_LENGTH" TYPE="INTEGER" DEFAULT_VALUE="32" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="CARRY_CHAIN" TYPE="STRING" VALUE="MANUAL" DEFAULT_VALUE_INDEX="0" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="CASCADE_CHAIN" TYPE="STRING" VALUE="MANUAL" DEFAULT_VALUE_INDEX="0" CONTEXT="SYNTH_ONLY" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="lpm_and" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="data" TYPE="INPUT" WIDTH="LPM_SIZE" WIDTH2D="LPM_WIDTH" />
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="LPM_WIDTH" />
	</PORTS>
</ROOT>
