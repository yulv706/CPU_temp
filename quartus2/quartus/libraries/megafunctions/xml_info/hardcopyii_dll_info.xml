		<PARAMETER NAME="DELAY_CHAIN_LENGTH" TYPE="INTEGER" DEFAULT_VALUE="16" />
		<PARAMETER NAME="DELAYCTRLOUT_MODE" TYPE="STRING" VALUE="normal|normal_offset|offset_only|static|test" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INPUT_FREQUENCY" TYPE="STRING" />
