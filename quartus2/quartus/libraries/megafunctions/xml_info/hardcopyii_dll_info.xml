		<PORT NAME="dqsupdate" TYPE="OUTPUT" />
		<PORT NAME="offset" TYPE="INPUT" WIDTH="6" DEFAULT_VALUE="0" />
		<PORT NAME="offsetctrlout" TYPE="OUTPUT" WIDTH="6" />
		<PORT NAME="upndnin" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="upndninclkena" TYPE="INPUT" DEFAULT_VALUE="1" />
