<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altddio_in_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTDDIO_IN" WIZARD_NAME="ALTDDIO_IN" MODULE_NAME="altddio_in" >
			<PARAMETER NAME="WIDTH" CNX_TYPE="PRIVATE" TYPE="INTEGER" VALUE="WIDTH" />
			<PARAMETER NAME="ARESET_MODE" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="is_used(aclr) ? 0 : (is_used(aset) ? 1 : 2)"/>
			</PARAMETER>
			<PARAMETER NAME="POWER_UP_HIGH" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="POWER_UP_HIGH == ON ? 1 : 0"/>
			</PARAMETER>
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" CNX_TYPE="PRIVATE" TYPE="STRING" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="CLKEN" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="is_used(inclocken) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="INVERT_INPUT_CLOCKS" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="INVERT_INPUT_CLOCKS == ON ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="SRESET_MODE" CNX_TYPE="PRIVATE" TYPE="INTEGER" >
				<FORCE_VALUE TO="is_used(sclr) ? 0 : (is_used(sset) ? 1 : 2)"/>
			</PARAMETER>
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
