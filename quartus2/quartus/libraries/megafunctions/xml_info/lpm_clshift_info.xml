<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<HAS_SIM_LIB VALUE="ON" />
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="LPM_PIPELINE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="LPM_SHIFTTYPE" TYPE="STRING" VALUE="LOGICAL|ARITHMETIC|ROTATE" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="LPM_WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_WIDTHDIST" TYPE="INTEGER" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="lpm_clshift" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="clken" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="clock" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="data" TYPE="INPUT" WIDTH="LPM_WIDTH" />
		<PORT NAME="direction" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="distance" TYPE="INPUT" WIDTH="LPM_WIDTHDIST" />
		<PORT NAME="overflow" TYPE="OUTPUT" />
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="LPM_WIDTH" />
		<PORT NAME="underflow" TYPE="OUTPUT" />
	</PORTS>
</ROOT>
