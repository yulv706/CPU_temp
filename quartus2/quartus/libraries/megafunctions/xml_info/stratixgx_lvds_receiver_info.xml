<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="stratixgx" />
		<HAS_SIM_LIB VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="CHANNEL_WIDTH" TYPE="INTEGER" />
		<PARAMETER NAME="DPLL_LOCKCNT" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="DPLL_LOCKWIN" TYPE="INTEGER" DEFAULT_VALUE="100" />
		<PARAMETER NAME="DPLL_RAWPERROR" TYPE="STRING" VALUE="on|off" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_DPA" TYPE="STRING" VALUE="on|off" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ENABLE_FIFO" TYPE="STRING" VALUE="on|off" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="USE_ENABLE1" TYPE="STRING" VALUE="true|false" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="stratixgx_lvds_receiver" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="bitslip" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="clk0" TYPE="INPUT" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="coreclk" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="datain" TYPE="INPUT" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="dataout" TYPE="OUTPUT" WIDTH="CHANNEL_WIDTH" />
		<PORT NAME="dpalock" TYPE="OUTPUT" />
		<PORT NAME="dpareset" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="dpllreset" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="enable0" TYPE="INPUT" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="enable1" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="devclrn" TYPE="INPUT" DEFAULT_VALUE="1" CONTEXT="SIM_ONLY" />
		<PORT NAME="devpor" TYPE="INPUT" DEFAULT_VALUE="1" CONTEXT="SIM_ONLY" />
	</PORTS>
</ROOT>
