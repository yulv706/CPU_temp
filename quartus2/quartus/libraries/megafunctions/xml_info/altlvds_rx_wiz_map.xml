<?xml version="1.0"?>
<ROOT>
	<INCLUDE FILE="altlvds_rx_rules.xml" />
	<WIZARD_DATA>
		<WIZARD_MAP NAME="ALTLVDS" WIZARD_NAME="ALTLVDS" MODULE_NAME="altlvds_rx" >
			<PARAMETER NAME="Num_Channel" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="NUMBER_OF_CHANNELS" />
			<PARAMETER NAME="Deser_Factor" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="DESERIALIZATION_FACTOR" />
			<PARAMETER NAME="Enable_DPA_Mode" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="ENABLE_DPA_MODE" />
			<PARAMETER NAME="Ext_PLL" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="USE_EXTERNAL_PLL" />						
			<PARAMETER NAME="Le_Serdes" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="IMPLEMENT_IN_LES" />
			<PARAMETER NAME="LVDS_Mode" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="1" />
			<PARAMETER NAME="Reg_InOut" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="REGISTERED_OUTPUT == ON ? 1 : 0" />
			</PARAMETER>			
			<PARAMETER NAME="Clock_Mode" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="1" />
			<PARAMETER NAME="Use_Lock" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_locked) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="PLL_Enable" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_pll_enable) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Use_Deskew" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_deskew) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="PLL_Freq" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(1000000 double_div INCLOCK_PERIOD)" />
			</PARAMETER>
			<PARAMETER NAME="PLL_Period" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="INCLOCK_PERIOD double_div 1000" />
			</PARAMETER>
			<PARAMETER NAME="Int_Device" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INTENDED_DEVICE_FAMILY" />
			<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INTENDED_DEVICE_FAMILY" />			
			<PARAMETER NAME="Inclock_Boost" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="INCLOCK_BOOST" />
			<PARAMETER NAME="CDS_Mode" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="CDS_MODE == MULTIPLE_BIT ? 2 : (CDS_MODE == SINGLE_BIT ? 1 : 0)" />
			</PARAMETER>
			<PARAMETER NAME="Use_Common_Rx_Tx_Plls" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(COMMON_RX_TX_PLL == ON) ? 1 : 0" />
			</PARAMETER>	
			<PARAMETER NAME="Data_rate" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INPUT_DATA_RATE" />
			<PARAMETER NAME="PLL_OPERATION_MODE" TYPE="STRING" CNX_TYPE="CONSTANT" VALUE="PLL_OPERATION_MODE" />
			<PARAMETER NAME="Input_Clock_alignment" TYPE="STRING" CNX_TYPE="PRIVATE" VALUE="INCLOCK_DATA_ALIGNMENT" />
			<PARAMETER NAME="INCLOCK_PHASE_SHIFT" TYPE="STRING" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(INCLOCK_PHASE_SHIFT) ? INCLOCK_PHASE_SHIFT : UNUSED" />
			</PARAMETER>
			<PARAMETER NAME="RX_ALIGN_DATA_REG" TYPE="STRING" CNX_TYPE="CONSTANT" VALUE="RX_ALIGN_DATA_REG" />
			<PARAMETER NAME="Dpa_Locked" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_dpa_locked) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Dpll_Enable" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_dpll_enable) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Dpll_Hold" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_dpll_hold) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Dpll_Reset" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_dpll_reset) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Fifo_Reset" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_fifo_reset) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Reset_Fifo" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(RESET_FIFO_AT_FIRST_LOCK == ON) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Lose_Lock" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="(LOSE_LOCK_ON_ONE_CHANGE == ON) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Use_Data_Align" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_data_align) ? 1 : (is_used(rx_channel_data_align) ? 1 : 0)" />
			</PARAMETER>
			<PARAMETER NAME="Channel_Data_Align_Reset" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_cda_reset) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Channel_Data_Align_Max" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="is_used(rx_cda_max) ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Use_Ext_Data_Align" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="REGISTERED_DATA_ALIGN_INPUT == ON ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Enable_FIFO_DPA_Channels" TYPE="INTEGER" CNX_TYPE="PRIVATE" >
				<FORCE_VALUE TO="ENABLE_DPA_FIFO == ON ? 1 : 0" />
			</PARAMETER>
			<PARAMETER NAME="Bitslip" TYPE="INTEGER" CNX_TYPE="PRIVATE" VALUE="DATA_ALIGN_ROLLOVER" />
			<PARAMETER NAME="BUFFER_IMPLEMENTATION" TYPE="STRING" CNX_TYPE="CONSTANT" VALUE="BUFFER_IMPLEMENTATION" />
		</WIZARD_MAP>
	</WIZARD_DATA>
</ROOT>
