<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="DENORMAL_SUPPORT" TYPE="STRING" VALUE="YES|NO" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="DIRECTION" TYPE="STRING" VALUE="ADD|SUB|VARIABLE" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="EXCEPTION_HANDLING" TYPE="STRING" VALUE="YES|NO" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="OPTIMIZE" TYPE="STRING" VALUE="SPEED|AREA" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="PIPELINE" TYPE="INTEGER" DEFAULT_VALUE="11" />
		<PARAMETER NAME="REDUCED_FUNCTIONALITY" TYPE="STRING" VALUE="YES|NO" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="ROUNDING" TYPE="STRING" VALUE="TO_NEAREST" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="SPEED_OPTIMIZED" TYPE="STRING" VALUE="STRATIX_ONLY|NO|YES" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="WIDTH_EXP" TYPE="INTEGER" DEFAULT_VALUE="8" />
		<PARAMETER NAME="WIDTH_MAN" TYPE="INTEGER" DEFAULT_VALUE="23" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altfp_add_sub" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="aclr" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="add_sub" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="clk_en" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="clock" TYPE="INPUT" />
		<PORT NAME="dataa" TYPE="INPUT" WIDTH="WIDTH_EXP+WIDTH_MAN+1" />
		<PORT NAME="datab" TYPE="INPUT" WIDTH="WIDTH_EXP+WIDTH_MAN+1" />
		<PORT NAME="denormal" TYPE="OUTPUT" />
		<PORT NAME="indefinite" TYPE="OUTPUT" />
		<PORT NAME="nan" TYPE="OUTPUT" />
		<PORT NAME="overflow" TYPE="OUTPUT" />
		<PORT NAME="result" TYPE="OUTPUT" WIDTH="WIDTH_EXP+WIDTH_MAN+1" />
		<PORT NAME="underflow" TYPE="OUTPUT" />
		<PORT NAME="zero" TYPE="OUTPUT" />
	</PORTS>
</ROOT>
