<?xml version="1.0"?>
<ROOT>
	<FUNCTION_INFORMATION>
		<VHDL_LIBRARY NAME="altera_mf.altera_mf_components" />
		<HAS_SIM_LIB VALUE="ON" />
		<TREAT_DEFAULT_PORT_VALUE_AS_UNUSED VALUE="ON" />
	</FUNCTION_INFORMATION>
	<PARAMETERS>
		<PARAMETER NAME="CENTER_ALIGN_MSB" TYPE="STRING" VALUE="UNUSED|OFF|ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="COMMON_RX_TX_PLL" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="CORECLOCK_DIVIDE_BY" TYPE="INTEGER" DEFAULT_VALUE="2" />
		<PARAMETER NAME="DESERIALIZATION_FACTOR" TYPE="INTEGER" DEFAULT_VALUE="4" />
		<PARAMETER NAME="INTENDED_DEVICE_FAMILY" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="DIFFERENTIAL_DRIVE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="ENABLE_CLK_LATENCY" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="0" LPM_HINT="ON" />
		<PARAMETER NAME="IMPLEMENT_IN_LES" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INCLOCK_BOOST" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="INCLOCK_DATA_ALIGNMENT" TYPE="STRING" VALUE="EDGE_ALIGNED|45_DEGREES|90_DEGREES|135_DEGREES|180_DEGREES|225_DEGREES|270_DEGREES|315_DEGREES|CENTER_ALIGNED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="INCLOCK_PERIOD" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="INCLOCK_PHASE_SHIFT" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="MULTI_CLOCK" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="NUMBER_OF_CHANNELS" TYPE="INTEGER" />
		<PARAMETER NAME="OUTCLOCK_ALIGNMENT" TYPE="STRING" VALUE="EDGE_ALIGNED|45_DEGREES|90_DEGREES|135_DEGREES|180_DEGREES|225_DEGREES|270_DEGREES|315_DEGREES|CENTER_ALIGNED" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="OUTCLOCK_DIVIDE_BY" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="OUTCLOCK_DUTY_CYCLE" TYPE="INTEGER" DEFAULT_VALUE="50" />
		<PARAMETER NAME="OUTCLOCK_IMPLEMENTATION" TYPE="STRING" VALUE="JITTER_CONTROL|SKEW_CONTROL" DEFAULT_VALUE_INDEX="1" LPM_HINT="ON" />
		<PARAMETER NAME="OUTCLOCK_MULTIPLY_BY" TYPE="INTEGER" DEFAULT_VALUE="1" />
		<PARAMETER NAME="OUTCLOCK_PHASE_SHIFT" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="OUTCLOCK_RESOURCE" TYPE="STRING" VALUE="AUTO|Global Clock|Regional Clock" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="OUTPUT_DATA_RATE" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="PLL_BANDWIDTH_TYPE" TYPE="STRING" VALUE="AUTO|HIGH|MEDIUM|LOW" DEFAULT_VALUE_INDEX="0" LPM_HINT="ON" />
		<PARAMETER NAME="PLL_SELF_RESET_ON_LOSS_LOCK" TYPE="STRING" VALUE="ON|OFF" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="PREEMPHASIS_SETTING" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="REGISTERED_INPUT" TYPE="STRING" VALUE="OFF|ON|TX_CLKIN|TX_CORECLK" DEFAULT_VALUE_INDEX="1" />
		<PARAMETER NAME="USE_EXTERNAL_PLL" TYPE="STRING" VALUE="OFF|ON" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="USE_NO_PHASE_SHIFT" TYPE="STRING" VALUE="ON|OFF" DEFAULT_VALUE_INDEX="0" />
		<PARAMETER NAME="VOD_SETTING" TYPE="INTEGER" DEFAULT_VALUE="0" />
		<PARAMETER NAME="LPM_HINT" TYPE="STRING" VALUE="UNUSED" />
		<PARAMETER NAME="LPM_TYPE" TYPE="STRING" VALUE="altlvds_tx" />
		<PARAMETER NAME="clk_src_is_pll" TYPE="STRING" VALUE="off" DEFAULT_VALUE_INDEX="0" CONTEXT="SIM_ONLY" />
	</PARAMETERS>
	<PORTS>
		<PORT NAME="pll_areset" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="sync_inclock" TYPE="INPUT" DEFAULT_VALUE="0" />
		<PORT NAME="tx_coreclock" TYPE="OUTPUT" />
		<PORT NAME="tx_enable" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="tx_in" TYPE="INPUT" WIDTH="DESERIALIZATION_FACTOR*NUMBER_OF_CHANNELS" />
		<PORT NAME="tx_inclock" TYPE="INPUT" SIM_DEFAULT_VALUE="0" />
		<PORT NAME="tx_locked" TYPE="OUTPUT" />
		<PORT NAME="tx_out" TYPE="OUTPUT" WIDTH="NUMBER_OF_CHANNELS" />
		<PORT NAME="tx_outclock" TYPE="OUTPUT" />
		<PORT NAME="tx_pll_enable" TYPE="INPUT" DEFAULT_VALUE="1" />
		<PORT NAME="tx_syncclock" TYPE="INPUT" DEFAULT_VALUE="0" />
	</PORTS>
</ROOT>
