						<MESSAGE_ARGUMENT NAME="ARG5" TYPE="NODE" VALUE="NODE_1"/>						
					</MESSAGE>						
				</MESSAGE>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
</DA_RULE>

