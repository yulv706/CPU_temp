<DA_RULE ID="C101" SEVERITY="CRITICAL" NAME="Gated clock should be implemented according to the Altera standard scheme" DEFAULT_RUN="YES" >	
	<REPORTING_ROOT>
		<MESSAGE NAME="(%ARG0%) Rule %ARG1%: %ARG2%. Found %ARG3% node(s) related to this rule.">
			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="COMB_1"/>
			<MESSAGE NAME="Node: %ARG4%" >
				<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="COMB_1"/>				
				<MESSAGE NAME="Gated clock destination node: %ARG5%" >			
					<MESSAGE_ARGUMENT NAME="ARG5" TYPE="NODE" VALUE="REG_1"/>				
				</MESSAGE>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION> 
		<DECLARE>
			<!-- Node declaration -->			
			<NODE NAME="COMB_1" TYPE="COMB,!BUF,!DQS" />
			<NODE NAME="REG_1" TYPE="REG" />
			<NODE NAME="REG_2" TYPE="REG" />			
		</DECLARE>
		
		<FORBID>
			<NODE_RELATIONSHIP FROM_NAME="COMB_1" TO_NAME="REG_1" TO_PORT="CLK_PORT|INV_CLK_PORT"/>
			<FORBID_EXCEPTION>
				<!-- Local Node declaration -->			
				<NODE NAME="COMB_1" TOTAL_FANIN="EQ2" />
				
				<OR>
					<AND>
						<NODE_RELATIONSHIP FROM_NAME="COMB_1" FROM_TYPE="AND,!BUF,!DQS" TO_NAME="REG_1" TO_PORT="CLK_PORT,!INV_CLK_PORT" />
						<NODE_RELATIONSHIP FROM_NAME="REG_2" TO_NAME="COMB_1"/>
						<NODE_RELATIONSHIP FROM_NAME="CLK_SRC" TO_NAME="REG_2" TO_PORT="INV_CLK_PORT"/>
						<NODE_RELATIONSHIP FROM_NAME="CLK_SRC" TO_NAME="COMB_1" />
					</AND>
					<AND>
						<NODE_RELATIONSHIP FROM_NAME="COMB_1" FROM_TYPE="OR,!BUF,!DQS" TO_NAME="REG_1" TO_PORT="INV_CLK_PORT,!CLK_PORT" />
						<NODE_RELATIONSHIP FROM_NAME="REG_2" TO_NAME="COMB_1"/>
						<NODE_RELATIONSHIP FROM_NAME="CLK_SRC" TO_NAME="REG_2" TO_PORT="CLK_PORT"/>
						<NODE_RELATIONSHIP FROM_NAME="CLK_SRC" TO_NAME="COMB_1" />					
					</AND>
				</OR>
			</FORBID_EXCEPTION>
		</FORBID>		
	</RULE_DEFINITION>
</DA_RULE>

