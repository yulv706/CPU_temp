<DA_RULE ID="A105" SEVERITY="CRITICAL" NAME="Pulses should not be implemented asynchronously" DEFAULT_RUN="YES" >	
	<REPORTING_ROOT>
		<MESSAGE NAME="(%ARG0%) Rule %ARG1%: %ARG2%. Found %ARG3% pulse generator(s) related to this rule.">
			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="NODE_2"/>
			<MESSAGE NAME="Pulse generator consists of:" >				
				<MESSAGE NAME="Nodes: %ARG4%" >
					<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="NODE_2"/>
				</MESSAGE>
				<MESSAGE NAME="Source Node: %ARG5%" >			
					<MESSAGE_ARGUMENT NAME="ARG5" TYPE="NODE" VALUE="NODE_1"/>				
				</MESSAGE>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION>		
		<FORBID>
			<OR>
				<AND>
					<NODE NAME="NODE_2" TOTAL_FANIN="EQ2"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="!BUF, !INV" TO_NAME="NODE_2" TO_TYPE="AND|NAND|OR|NOR" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ1" REQUIRED_THROUGH="YES"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="!BUF, !INV" TO_NAME="NODE_2" TO_TYPE="AND|NAND|OR|NOR" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ0"/>
				</AND>
				<NODE NAME="NODE_2" TYPE="REG"/>
				<NODE_RELATIONSHIP FROM_NAME="NODE_2" TO_NAME="NODE_2" TO_TYPE="REG" TO_PORT="INV_CLR_PORT|PRESET_PORT" THROUGH_TYPE="BUF|INV" REQUIRED_THROUGH="YES" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ1"/>
				<NODE_RELATIONSHIP FROM_NAME="NODE_2" TO_NAME="NODE_2" TO_TYPE="REG" TO_PORT="CLR_PORT|INV_PRESET_PORT" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ0"/>
			</OR>
		</FORBID>		
	</RULE_DEFINITION>
</DA_RULE>
<DA_RULE ID="A106" SEVERITY="CRITICAL" NAME="Multiple pulses should not be generated in design" DEFAULT_RUN="YES" >	
	<REPORTING_ROOT>
		<MESSAGE NAME="(%ARG0%) Rule %ARG1%: %ARG2%. Found %ARG3% structure(s) related to this rule.">
			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="NODE_1"/>
			<MESSAGE NAME="Multiple generator structure" >
				<MESSAGE NAME="Nodes: %ARG4%" >
					<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="NODE_1"/>
				</MESSAGE>
				<MESSAGE NAME="Source Node: %ARG5%" >			
					<MESSAGE_ARGUMENT NAME="ARG5" TYPE="NODE" VALUE="NODE_2"/>				
				</MESSAGE>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION> 
		<DECLARE>
			<!-- Node declaration -->
			<NODE NAME="NODE_1" TOTAL_FANIN="EQ2"/>
		</DECLARE>
		<FORBID>
			<OR>			
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="AND" TO_NAME="NODE_1" TO_TYPE="AND" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ1" REQUIRED_THROUGH="YES"/>			
					<NODE_RELATIONSHIP FROM_NAME="NODE_2" FROM_TYPE="!BUF,!INV" TO_NAME="NODE_1" TO_TYPE="AND" />
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="OR" TO_NAME="NODE_1" TO_TYPE="OR" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ1" REQUIRED_THROUGH="YES"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_2" FROM_TYPE="!BUF,!INV" TO_NAME="NODE_1" TO_TYPE="OR" />
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="NAND" TO_NAME="NODE_1" TO_TYPE="NAND" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ0"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_2" FROM_TYPE="!BUF,!INV" TO_NAME="NODE_1" TO_TYPE="NAND" />
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="NOR" TO_NAME="NODE_1" TO_TYPE="NOR" THROUGH_TYPE="BUF|INV" THROUGH_COUNT_TYPE="INV" THROUGH_NODE_COUNT="MOD2EQ0"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_2" FROM_TYPE="!BUF,!INV" TO_NAME="NODE_1" TO_TYPE="NOR" />
				</AND>
			</OR>
			
		</FORBID>		
	</RULE_DEFINITION>
</DA_RULE>
<DA_RULE ID="A107" SEVERITY="CRITICAL" NAME="Design should not contain SR latches" DEFAULT_RUN="YES" >	
	<REPORTING_ROOT>
		<MESSAGE NAME="(%ARG0%) Rule %ARG1%: %ARG2%. Found %ARG3% SR latche(s) related to this rule.">
			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="NODE_1"/>
			<MESSAGE NAME="SR latch structure" >				
				<MESSAGE NAME="Node: %ARG4%" >
					<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="NODE_1"/>
				</MESSAGE>
				<MESSAGE NAME="Node: %ARG5%" >			
					<MESSAGE_ARGUMENT NAME="ARG5" TYPE="NODE" VALUE="NODE_2"/>				
				</MESSAGE>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION> 
		<DECLARE>
			<!-- Node declaration -->					
		</DECLARE>
		<FORBID>
			<OR>
				<NODE NAME="NODE_1" TYPE="SRLATCH"/>
				<HAS_NODE NODE_LIST="NODE_1"/>
				<NODE NAME="NODE_1" TOTAL_FANIN="EQ2"/>
				<NODE NAME="NODE_2" TOTAL_FANIN="EQ2"/>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="NAND" TO_NAME="NODE_2" TO_TYPE="NAND" />
					<NODE_RELATIONSHIP FROM_NAME="NODE_2" FROM_TYPE="NAND" TO_NAME="NODE_1" TO_TYPE="NAND" />					
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="NOR" TO_NAME="NODE_2" TO_TYPE="NOR" />
					<NODE_RELATIONSHIP FROM_NAME="NODE_2" FROM_TYPE="NOR" TO_NAME="NODE_1" TO_TYPE="NOR" />					
				</AND>
				
			</OR>
		</FORBID>		
	</RULE_DEFINITION>
</DA_RULE>

