				<AND>
					<NODE_RELATIONSHIP FROM_NAME="REG_3" TO_NAME="REG_2" TO_PORT="D_PORT" CLOCK_RELATIONSHIP="SYNC |COIN_EDGE|ALL_EDGES"/>					
					<SAME_CLOCK_SOURCE NODE_LIST="REG_2, REG_3" />
					<NODE_RELATIONSHIP FROM_NAME="REG_4" TO_NAME="REG_3" TO_PORT="D_PORT" REQUIRED_THROUGH="YES" THROUGH_TYPE="COMB" CLOCK_RELATIONSHIP="ASYN"/>					 
				</AND>				
			</OR>
		</FORBID>
	</RULE_DEFINITION>
</DA_RULE>

