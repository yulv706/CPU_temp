<DA_RULE ID="S103" SEVERITY="HIGH" NAME="More than one asynchronous signal port of the same register should not be driven by the same signal source" DEFAULT_RUN="YES" >	
	
	<RULE_DEFINITION> 
		<DECLARE>			
			<NODE NAME="REG_1" TYPE="REG"/>			
			<NODE NAME="NODE_1" TYPE="!COMB"/>
		</DECLARE>
		<FORBID>
			<OR>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="CLR_PORT" THROUGH_TYPE="COMB"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="RESET_PORT,!CLR_PORT" THROUGH_TYPE="COMB"/>
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="INV_CLR_PORT" THROUGH_TYPE="COMB"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="RESET_PORT,!INV_CLR_PORT" THROUGH_TYPE="COMB"/>
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="PRE_PORT" THROUGH_TYPE="COMB"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="RESET_PORT,!PRE_PORT" THROUGH_TYPE="COMB"/>
				</AND>
				<AND>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="INV_PRE_PORT" THROUGH_TYPE="COMB"/>
					<NODE_RELATIONSHIP FROM_NAME="NODE_1" TO_NAME="REG_1" TO_PORT="RESET_PORT,!INV_PRE_PORT" THROUGH_TYPE="COMB"/>
				</AND>

			</OR>
		</FORBID>		
	</RULE_DEFINITION>
	<REPORTING_ROOT>
