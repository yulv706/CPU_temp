			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>		
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="PIN_1"/>
			<MESSAGE NAME="Node: %ARG4%" >
				<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="PIN_1"/>
				<MESSAGE NAME="Reset signal destination node(s) list">
					<MESSAGE NAME="Node: %ARG5%" >			
						<MESSAGE_ARGUMENT NAME="ARG5" TYPE="NODE" VALUE="REG_1"/>				
					</MESSAGE>						
				</MESSAGE>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION> 
		<DECLARE>			
			<NODE NAME="PIN_1" TYPE="PIN"/>
			<NODE NAME="REG_1" TYPE="REG"/>
			<NODE NAME="REG_2" TYPE="REG"/>
			<NODE NAME="REG_3" TYPE="REG"/>
		</DECLARE>		
		
		<FORBID>
			<NODE_RELATIONSHIP FROM_NAME="PIN_1" TO_NAME="REG_1" TO_PORT="RESET_PORT"/>
			<FORBID_EXCEPTION>				
				<OR>
					<AND>
						<NODE NAME="REG_1" TYPE="REG" DATA_IN_VCC="YES"/>
						<NODE_RELATIONSHIP FROM_NAME="PIN_1" TO_NAME="REG_1" TO_PORT="INV_CLR_PORT" />
						<NODE_RELATIONSHIP FROM_NAME="PIN_1" TO_NAME="REG_2" TO_PORT="INV_CLR_PORT" />
						<NODE_RELATIONSHIP FROM_NAME="REG_1" TO_NAME="REG_2" TO_PORT="D_PORT" CLOCK_RELATIONSHIP="SYNC |COIN_EDGE|ALL_EDGES"/>
						<SAME_CLOCK_SOURCE NODE_LIST="REG_1, REG_2" />						
						<NODE_RELATIONSHIP FROM_NAME="REG_2" TO_NAME="REG_3" TO_PORT="INV_CLR_PORT" />
						<SAME_CLOCK_SOURCE NODE_LIST="REG_2, REG_3" />
					</AND>
					<AND>
						<NODE NAME="REG_1" TYPE="REG" DATA_IN_GND="YES"/>
						<NODE_RELATIONSHIP FROM_NAME="PIN_1" TO_NAME="REG_1" TO_PORT="PRE_PORT" />
						<NODE_RELATIONSHIP FROM_NAME="PIN_1" TO_NAME="REG_2" TO_PORT="PRE_PORT" />
						<NODE_RELATIONSHIP FROM_NAME="REG_1" TO_NAME="REG_2" TO_PORT="D_PORT" CLOCK_RELATIONSHIP="SYNC |COIN_EDGE|ALL_EDGES"/>
						<SAME_CLOCK_SOURCE NODE_LIST="REG_1, REG_2" />						
						<NODE_RELATIONSHIP FROM_NAME="REG_2" TO_NAME="REG_3" TO_PORT="CLR_PORT" />
						<SAME_CLOCK_SOURCE NODE_LIST="REG_2, REG_3" />
					</AND>
				</OR>
			</FORBID_EXCEPTION>
		</FORBID>
	</RULE_DEFINITION>
</DA_RULE>

