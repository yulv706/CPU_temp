<DA_RULE ID="A103" SEVERITY="HIGH" NAME="Design should not contain delay chains" DEFAULT_RUN="YES" >	
	<REPORTING_ROOT>
		<MESSAGE NAME="(%ARG0%) Rule %ARG1%: %ARG2%. Found %ARG3% delay chain related to this rule.">
			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="NODE_1"/>
			<MESSAGE NAME="Nodes: %ARG4%" >			
				<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="NODE_1"/>				
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION> 
		<DECLARE>
			<NODE NAME="NODE_1" TOTAL_FANIN="GT0"/>
			<NODE NAME="NODE_2" TYPE="REG"/>
		</DECLARE>
		<FORBID>
			<OR>
				<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="BUF,!TRI_STATE, !CARRY_CASCADE_CHAIN" TO_NAME="NODE_2" TO_PORT="CLK_PORT|INV_CLK_PORT|RESET_PORT" THROUGH_TYPE="BUF,!TRI_STATE, !CARRY_CASCADE_CHAIN"/>
				<NODE_RELATIONSHIP FROM_NAME="NODE_1" FROM_TYPE="INV,!TRI_STATE, !CARRY_CASCADE_CHAIN" TO_NAME="NODE_2" TO_PORT="CLK_PORT|INV_CLK_PORT|RESET_PORT" THROUGH_TYPE="INV,!TRI_STATE, !CARRY_CASCADE_CHAIN" THROUGH_COUNT_TYPE="INV" REQUIRED_THROUGH="YES" THROUGH_NODE_COUNT="MOD2EQ0"/>				
			</OR>	
			
		</FORBID>		
	</RULE_DEFINITION>
</DA_RULE>

