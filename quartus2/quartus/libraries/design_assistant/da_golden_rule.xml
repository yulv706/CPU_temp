			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>		
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="REG_1"/>
			<MESSAGE NAME="Node %ARG4%" >
				<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="REG_1"/>
				<MESSAGE NAME="Violated asynchronous ports source node(s) list">
