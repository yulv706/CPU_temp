<DA_RULE ID="A101" SEVERITY="CRITICAL" NAME="Design should not contain combinational loops" DEFAULT_RUN="YES" >	
	<REPORTING_ROOT>
		<MESSAGE NAME="(%ARG0%) Rule %ARG1%: %ARG2%. Found %ARG3% nodes with combinational loop related to this rule.">
			<MESSAGE_ARGUMENT NAME="ARG0" TYPE="ATTRIBUTE" VALUE="SEVERITY"/>
			<MESSAGE_ARGUMENT NAME="ARG1" TYPE="ATTRIBUTE" VALUE="ID"/>
			<MESSAGE_ARGUMENT NAME="ARG2" TYPE="ATTRIBUTE" VALUE="NAME"/>
			<MESSAGE_ARGUMENT NAME="ARG3" TYPE="TOTAL_NODE" VALUE="COMB_1"/>
			<MESSAGE NAME="Comb loop " >				
				<MESSAGE NAME="Nodes: %ARG4%" >			
					<MESSAGE_ARGUMENT NAME="ARG4" TYPE="NODE" VALUE="COMB_1"/>				
				</MESSAGE>
			</MESSAGE>
		</MESSAGE>
			
	</REPORTING_ROOT>
	<RULE_DEFINITION> 
		<DECLARE>			
			<NODE NAME="COMB_1" TYPE="COMB" />
		</DECLARE>
		<FORBID>
			<HAS_LOOP NAME="COMB_1" TYPE="COMB"/>
		</FORBID>		
	</RULE_DEFINITION>
</DA_RULE>

