SYSTEM_DEFAULTS
{
   WIZARD_SCRIPT_ARGUMENTS
   {
      device_family = "STRATIX";
      clock_freq = "50000000";
      generate_hdl = "1";
      generate_sdk = "1";
      do_build_sim = "1";
      hardcopy_compatible = "0";
      board_class = "";
      CLOCKS
      {
      	CLOCK clk 
      	{
      		frequency = "50000000";
      		source = "External";
      	}
        clk = "50000000";
      }
   }
}

USER_INTERFACE
{
   WIZARD_UI main
   {
      title = "Altera SOPC Builder - {{ $$system_name; }}";
      java = "sopc_builder.sopc_builder";
      help = "http://www.altera.com/literature/hb/qts/qts_qii5v4.pdf";
      icon = "sopc16.gif";
      split = "vertical";
      ACTION initialize
      {
         # true if on windows
         $$onwin_32 = "{{ $$os == 'win32' }}";
         # set initial family ID to be equal to family if not set
         init_device_family = "{{ if ($SYS/device_family_id == '') $SYS/device_family_id = $SYS/device_family; }}";
      }
      MENU
      {
         title = "File";
         key = "F";
         ITEM
         {
            title = "New System...";
            key = "N";
            action = "new_system";
         }
         ITEM
         {
            title = "Open System...";
            key = "O";
            action = "open_system";
            separator = "1";
         }
         ITEM
         {
             title = "New Component...";
             key = "C";
             action = "component_builder";
         }
         ITEM
         {
             title = "New Board Description...";
             key = "D";
             action = "new_board_description";
         }
         ITEM
         {
             title = "Edit Board Description...";
             key = "E";
             action = "edit_board_description";
             enable = "{{ $WSA/board_class != '' }}";
         }
         ITEM
         {
             title = "Check Web for Updates...";
             key = "W";
             action = "sopc_library_update";
         }
         ITEM
         {
             title = "Refresh Component List";
             key = "R";
             action = "update_pool";
             separator = "1";
         }
         ITEM
         {
            title = "Browse Project Directory...";
            key = "B";
            url = "file://{{ $$system_directory }}/";
         }
         ITEM
         {
            title = "SOPC Builder Setup...";
            key = "S";
            action = "edit_configuration";
            separator = "1";
         }
         ITEM
         {
            title = "Exit";
            key = "x";
            action = "wizard_cancel";
            separator = "0";
         }
      }
      $$hide_sdk_checkbox = "0";
      PAGES main
      {
         enable = "10";
         # current page displayed
         select = "1";
         PAGE 1
         {
            # title of page.
            title = "System Contents";
            # stack things side-by-side
            layout = "horizontal";
            # use a horizontally-split pane
            split = "horizontal";
            # always enabled.
            enable = "10";
            # peripheral pool & related UI
            GROUP pool
            {
               width = "188";
               height = "384";
               align = "center";
               spacing = "5";

               POOL tree
               {
                  java = "sopc_builder.sopc_module_pool";
                  width = "188";
                  height = "1000";
                  title = "Select a peripheral.";
                  REM MENU
                  {
                     title = "System";
                     key = "y";
                     ITEM
                     {
                        title = "Add Selected Module...";
                        action = "new_module";
                        key = "A";
                     }
                  }
               }
               PAGES pools
               {
                    tabs="bottom";
                    wrap=1;
                    width=2000;
                    height= "{{ if ($$os == 'win32') 40; else 65; }}";
                    select="all";
                    ACTION initialize
                    {
                        $../select = "{{ $CFG/sopc_pool_filter; }}";
                    }
                   PAGE all
                   {
                        icon="all.gif";
                        action="pool_filter_all";
                        TEXT 
                        { 
                            title="<small>&nbsp;All Available Components&nbsp;</small>"; 
                            tooltip=
                                "Showing components installed on this system,<br>
                                 and components downloadable via the internet";
                        }
                   }
                   PAGE install
                   {
                        icon="full_eval.gif";
                        action="pool_filter_install";
                        TEXT 
                        { 
                            title="<small>&nbsp;Installed Components&nbsp;</small>"; 
                            tooltip=
                                "Showing components which are currently installed<br>
                                 and available for use on this system"; 
                        }
                   }
                   PAGE updates
                   {
                        icon="all_updated.gif";
                        action="pool_filter_updates";
                        TEXT 
                        { 
                            title="<small>&nbsp;Web-Available Updates&nbsp;</small>"; 
                            tooltip=
                                "Showing newly available components which are <br>
                                 available for download via the internet";
                        }
                   }
                   PAGE catalog
                   {
                        icon="unlicensed.gif";
                        action="pool_filter_catalog";
                        TEXT 
                        { 
                            title="<small>&nbsp;Web-Available Components&nbsp;</small>"; 
                            tooltip=
                                "Showing components only available via download<br>
                                 or purchase (not installed on this system)"; 
                        }
                   }
                   # PAGE disabled for now...
                   REM target
                   {
                        icon="target.gif";
                        action="pool_filter_target";
                        TEXT 
                        { 
                            title="<small>&nbsp;Target Boards&nbsp;</small>"; 
                            tooltip=
                                "Showing target board components"; 
                        }
                   }
               }

               GROUP buttons
               {
                  layout = "flow";
                  spacing = "2";
                  BUTTON add
                  {
                     title = "Add...";
                     action = "new_module";
                     tooltip = "Add the selected component to your system as a new module";
                     key = "A";
                  }
                  BUTTON web_update
                  {
                     action = "sopc_library_update";
                     title = "Check";
                     tooltip = "Check Altera's website for updates and newly available components...";
                     icon = "wwwglobe.gif";
                  }
               }
               GROUP
               {
                  #Just for spacing
               }
            }
            # module table & related UI
            GROUP modules
            {
               width = "640";
               height = "384";
               align = "center";
               #spacing = "2";
               #split = "vertical";
               REM TREETABLE
               {
               		#java = "sopc_builder.sopc_heirarchy_table";
               		
               }
               
			   GROUP top
			   {
			      glue = 0;
                  layout = "flow";
                  include = "target_board";
               GROUP bus
               {
                  layout = "flow";
				GROUP target
				{
					align = "right";
						visible = "";
					title = "{{ if ($$show_board_components) 'Target'; }}";
					spacing = 4;
					COMBO board
					{
#						glue = "0";
						id = "board_id";
						title = "Board:";
						columns = 35;
						action = "set_board";
						visible = "{{ $$show_board_components }}";
						tooltip = "Select target board from list or choose<br>\"Unspecified\" to select device <br>family directly";
						DATA
						{
							board_class = "$";
						}
						ITEM unspecified
						{
						   title = "Unspecified Board";
						   value = "";
						   fixed = "1";
						}
					}
					GROUP family_selection
					{
						layout = "flow";
                          COMBO
                           {
                              #width = "128";
#                              glue = "0";
                              id = "set_device";
                              title = "Device Family:";
                              tooltip = "Select target device family.<br><b>NOTE:</b>  The device family selected in Quartus must match <br>this selection for correct operation.";
                              enable = "{{ $WSA/board_class == '' || !$$show_board_components; }}";
                              action = "set_device";
                              ITEM
                              {
                                 title = "Stratix III";
                                 value = "STRATIXIII";
                              }
                              ITEM
                              {
                                 title = "Stratix II";
                                 value = "STRATIXII";
                              }
                              ITEM
                              {
                                 title = "Stratix II GX";
                                 value = "STRATIXIIGX";
                                 visible = "{{ exists($$quartus_dir+'/common/devinfo/stratixiigx') }}";
                              }
                              ITEM
                              {
                                 title = "Arria GX";
                                 value = "STRATIXIIGXLITE";
                                 visible = "{{ exists($$quartus_dir+'/common/devinfo/arriagx') }}";
                              }
                              ITEM
                              {
                                 title = "Stratix";
                                 value = "STRATIX";
                              }
                              ITEM
                              {
                                 title = "Stratix GX";
                                 value = "STRATIXGX";
                                 visible = "{{ exists($$quartus_dir+'/common/devinfo/stratixgx') }}";
                              }
                              ITEM
                              {
                                 title = "Cyclone";
                                 value = "CYCLONE";
                              }
                              ITEM
                              {
                                 title = "Cyclone II";
                                 value = "CYCLONEII";
                              }
                              ITEM
                              {
                                 title = "Cyclone III";
                                 value = "CYCLONEIII";
                                 visible = "{{ exists($$quartus_dir+'/common/devinfo/cycloneiii') }}";
                              }
                              ITEM
                              {
                                 title = "MAX II";
                                 value = "MAXII";
                              }
                              ITEM
                              {
                                 title = "APEX 20K";
                                 value = "APEX20K";
                              }
                              ITEM
                              {
                                 title = "APEX 20KE";
                                 value = "APEX20KE";
                              }
                              ITEM
                              {
                                 title = "APEX 20KC";
                                 value = "APEX20KC";
                              }
                              ITEM
                              {
                                 title = "APEX II";
                                 value = "APEXII";
                              }
                              ITEM
                              {
                                 title = "ACEX 1K";
                                 value = "ACEX1K";
                              }
                              ITEM
                              {
                                 title = "FLEX 10KE";
                                 value = "FLEX10KE";
                              }
                              ITEM
                              {
                                 title = "Excalibur";
                                 value = "EXCALIBUR_ARM";
                                 visible = 0;
                              }
                              ITEM
                              {
                                 title = "Mercury";
                                 value = "MERCURY";
                              }
                              DATA
                              {
                                 device_family_id = "$";
                              }
                           }
						CHECK
						{
							id = "hardcopy_compatible";
							title = "HardCopy Compatible";
							enable = "{{ device_info('has_HC') > 0 }}";
							tooltip = "{{ 
								if ($enable) 'Limit generated logic to HardCopy resources.'; 
								        else 'HardCopy compatibility does not apply to the selected Device Family.';
								        }}";
							DATA
							{
								hardcopy_compatible = "$";
							}
						}
                      } # end GROUP family_selection
                      } # end GROUP target
                           #GROUP spacer { }
                     GROUP clocks
                     {
                         spacing = 3;
	                     TABLE
	                     {
	                     	java = "sopc_builder.sopc_clock_table";
	                     }
                  	}
                  #	GROUP spacer { width=1; height=8; }
               	}
               }
               TABLE module_table
               {
                  java = "sopc_builder.sopc_module_table";
                  width = "2048";
                  height = "2048";
                  MENU
                  {
                     title = "Module";
                     # appears on menu-bar
                     key = "M";
                     ITEM
                     {
                        title = "Edit...";
                        action = "edit_module";
                        key = "E";
                     }
                     ITEM
                     {
                        title = "Rename";
                        key = "R";
                        action = "rename_module";
                     }
                     ITEM
                     {
                        title = "Delete";
                        key = "D";
                        action = "delete_module";
                     }
                     ITEM
                     {
                        title = "Lock Base Address";
                        key = "L";
                        action = "lock_base_address";
                        type = "boolean";
                        separator = "1";
                     }
                     ITEM
                     {
                        title = "Move to top";
                        action = "module_top";
                  }
                     ITEM
                     {
                        title = "Move up";
                        action = "module_up";
                     }
                     ITEM
                     {
                        title = "Move down";
                        action = "module_down";
                     }
                     ITEM
                     {
                        title = "Move to bottom";
                        action = "module_bottom";
                     }
                  }
                  MENU
                  {
                     title = "System";
                     ITEM
                     {
                        title = "Auto-Assign Base Addresses";
                        action = "auto_address";
                        key = "U";
                     }
                     ITEM
                     {
                        title = "Auto-Assign IRQs";
                        key = "I";
                        action = "auto_irq";
                     }
                  }
                  MENU
                  {
                     title = "View";
                     key = "V";
                     ITEM
                     {
                        title = "Show Connections";
                        key = "M";
                        action = "view_connections";
                        # makes this a check-box menu item
                        type = "boolean";
                        # state of check-box comes from:
                        value = "{{ $WSA/view_master_columns; }}";
                     }
                     ITEM
                     {
                        title = "Show Arbitration";
                        key = "P";
                        action = "view_arbitration";
                        # makes this a check-box menu item
                        type = "boolean";
                        # state of check-box comes from:
                        value = "{{ $WSA/view_master_priorities; }}";
                        separator = 1;
                     }
                     ITEM
                     {
                        title = "Show Bus Type";
                        key = "T";
                        action = "view_bus_type";
                        type = "boolean";
                        value = "{{ $WSA/bustype_column_width > 0; }}";
                     }
                     ITEM
                     {
                        title = "Show Clock Source";
                        key = "T";
                        action = "view_clock_source";
                        type = "boolean";
                        value = "{{ $WSA/clock_column_width > 0; }}";
                        separator = 1;
                     }
                     ITEM
                     {
                        title = "Expand All";
                        key = "x";
                        action = "expand_all";
                     }
                     ITEM
                     {
                        title = "Collapse All";
                        key = "C";
                        action = "collapse_all";
                     }
                  }
               }
               GROUP movers
               {
                  layout = "flow";
                  align = "center";
                  spacing = 10;
                  BUTTON moveup 
                  { 
                     icon="uparrow.gif"; 
                     action = "module_up"; 
                     title = "Move Up"; 
                  }
                  BUTTON movedn 
                  { 
                     icon="dnarrow.gif"; 
                     action = "module_down"; 
                     title = "Move Down"; 
                  }
               }
               GROUP
               {
                  #Just for spacing
               }
            }
         }
         PAGE generate
         {
            split = "vertical";
            title = "System Generation";
            enable = "10";
            # always enabled.
            GROUP settings
            {
            glue = 0;
	           	layout = "vertical";
        	   	 align = "left";
	            GROUP options
	            {
	               title = "Options";
	               align = "left";
	               spacing = "3";
	               GROUP
	               {
	               		id = "builder_component_options";
	               }
	               update_sdk_checkbox = "{{ if ($$hide_sdk_checkbox > 0) $WSA/generate_sdk = 0; }}";
	               CHECK sdk_options
	               {
	                  title = "SDK.  Generate header files, library files, and memory contents for CPU(s) and peripherals in your system.";
	                  tooltip = "Produces subdirectories named for each CPU in your system: <b>{{ $$system_name }}_(cpu-name)_sdk/</b>";
	                  visible = "{{ $$hide_sdk_checkbox <= 0 }}";
	                  DATA
	                  {
	                     generate_sdk = "$";
	                  }
	               }
	               REM GROUP
	               {
	               	layout = "flow";
	               	align = "left";
	               	spacing = 3;
	               	 GROUP { width = 10; height = 10; }
				   }
	               CHECK hdl_options
	               {
	                  layout = "horizontal";
	                  title = "HDL.  Generate system module logic in {{ if ($WSA/hdl_language == 'verilog') 'Verilog'; else 'VHDL'; }}.";
	                  DATA
	                  {
	                     generate_hdl = "$";
	                  }
	                  jam = "1";
	                  REM TEXT
	                  {
	                     glue = "0";
	                     title = " device family.";
	                  }
	               }
	               GROUP sim_options
	               {
	                  indent = "20";
	                  CHECK
	                  {
	                     enable = "{{ $WSA/generate_hdl }}";
	                     title = "Simulation.  Create simulator project files.";
	                     tooltip = "Produces a subdirectory named <b>{{ $$system_name }}_sim</b>";
	                     DATA
	                     {
	                        do_build_sim = "$";
	                     }
	                     layout = "horizontal";
	                     jam = "1";
						 BUTTON
						 {
						    title = "Run Simulator";
						    glue = "0";
						    ACTION select
						    {
						       run = "{{ exec($$sopc_directory+'/bin/run_modelsim.pl', $CFG/sopc_simulator_dir+','+$$system_directory+','+$$system_name); }}";
						    }
						 }
	                  }
	               }	               
	           }
            }
            EDIT console
            {
               id = "generation_console";
               rows = "20";
               editable = "0";
               enable = "10";
            }
            message = "{{ if ($$generated == 0) 'Generation in-process; system is not editable.  Press the <b><u>S</u>top</b> button to make changes to your system.'; }}";
         }
         warning = "{{ if (!exists($$sopc_directory+'/.sopc_builder')) 'No valid configuration file present.  Please select <b>File->SOPC Builder Setup...</b> to setup configuration.'; }}";
         warning = "{{ if ($$os == 'unix' && $WSA/hdl_language == 'vhdl' && $$generated > 0) 
                    'You must add the file <b>altera_vhdl_support.vhd</b> to the top of your Quartus project file-list before compiling.'; }}";
      }
      MENU
      {
         title = "Help";
         key = "H";
         ITEM
         {
            title = "Accessing Help & Documentation...";
            #key = "A";
            icon = "help.gif";
            action = "menu_help_accessing";
         }
         ITEM
         {
            title = "SOPC Builder Handbook";
            icon = "doc.gif";
            url = "http://www.altera.com/literature/hb/qts/qts_qii5v4.pdf";
            separator = 1;
        }
         ITEM
         {
            title = "Online Documentation";
            url = "http://www.altera.com/literature/lit-sop.html#";
            icon = "wwwglobe.gif";
         }
         ITEM
         {
            title = "Altera Home Page";
            key = "H";
            url = "http://www.altera.com";
            icon = "html.gif";
         }
         ITEM
         {
            title = "SOPC Builder Home Page";
            key = "S";
            url = "http://www.altera.com/sopcbuilder";
            icon = "html.gif";
            separator = "1";
         }
         ITEM
         {
            title = "About Altera SOPC Builder...";
            key = "A";
            icon = "sopc16.gif";
            action = "menu_help_about";
         }
      }
      	
      ACTION menu_help_accessing
      {
         show_dialog = "{{ dialog('help_dialog', ''); }}";
      }
      ACTION menu_help_about
      {
         show_dialog = "{{ dialog('about_dialog', ''); }}";
      }
      ACTION edit_configuration
      {
         show_dialog = "{{ dialog('edit_configuration', '@'); }}";
      }
      GROUP messages
      {
         glue = 0;
         split = "{{ if ($CFG/sopc_ui_debug != 0) 'horizontal' }}";
         CONSOLE
         {
			visible = "{{ $CFG/sopc_ui_debug != 0 }}";
			enable = 10;
         	java = "sopc_wizard.sopc_ui_console";
         	rows = 5;
			font = "mono";
         }
         LIST
         {
            rows = 5;
            glue = 0;
            background = "lightGray";
         }
      }
      ACTION initialize
      {
         $$generated = "-1";
      }
      GROUP nav
      {
         layout = "horizontal";
         spacing = "16";
         BUTTON
         {
            title = "Exit";
            key = "x";
            action = "wizard_cancel";
         }
         BUTTON
         {
            title = "<  Prev";
            key = "P";
            action = "wizard_prev";
         }
         BUTTON
         {
            title = "Next  >";
            key = "N";
            action = "wizard_next";
         }
         BUTTON
         {
            title = " ";
            visible = "0";
         }
         # for spacing, since the next 3 buttons are crammed.
         BUTTON
         {
            title = "Re-Generate";
            key = "G";
            action = "builder_generate";
            enable = "{{ $$errors == 0 }}";
            visible = "{{ $$generated > 0 }}";
            cram = "1";
         }
         BUTTON
         {
            title = "Stop";
            key = "S";
            action = "builder_stop";
            visible = "{{ $$generated == 0 }}";
            cram = "1";
            enable = "10";
         }
         BUTTON
         {
            title = "Generate";
            key = "G";
            action = "builder_generate";
            enable = "{{ $$errors == 0 }}";
            visible = "{{ $$generated < 0 }}";
            cram = "1";
         }
      }


