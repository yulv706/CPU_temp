   WIZARD_UI new_dialog
   {
      title = "Create New System";
      align = "left";
#      spacing = "8";
      indent = "5";
      EDIT
      {
         columns = "15";
         title = "System Name: ";
         suffix = " ";
         type = "file";
         DATA
         {
            $$new_system_name = "$";
         }
      }
      GROUP
      {
         layout = "horizontal";
         title = "Target HDL";
         RADIO
         {
            title = "Verilog";
            DATA
            {
               $CFG/preferred_hdl = "verilog";
            }
         }
         RADIO
         {
            title = "VHDL";
            DATA
            {
               $CFG/preferred_hdl = "vhdl";
            }
         }
      }
      GROUP dialog
      {
        layout="horizontal";
        spacing=8;
        BUTTON cancel 
        { title="Cancel";  key="C"; action="dialog_cancel"; }
        BUTTON ok     
        { title="OK";      key="O"; default="1"; action="dialog_ok"; enable= "{{ $$new_system_name != '' }}";    }
      }
      GROUP messages
      {
      }
   }
   WIZARD_UI prompt_revert
   {
      TEXT
      {
         title = "Revert to previous System PTF?";
      }
   }
   WIZARD_UI edit_configuration
   {
      title = "SOPC Builder Setup";
      CONTEXT
      {
#         FILE = "@{{ $$sopc_directory }}/.sopc_builder";
         FILE = "$CFG";
         EDIT = "";
      }
      ACTION initialize
      {
         $$exe_suffix = "{{ if ($$onwin_32) '.exe'; }}";
         $$save_sopc_path = "{{ $EDIT/sopc_component_path }}";
         init_proxy_port = "{{ if (int($CFG/sopc_proxy_port) == 0) $CFG/sopc_proxy_port = 80; }}";
      }
      align = "left";
      GROUP
      {
         title = "Help and Documentation";
         align = "left";
         spacing = 4;
         COMBO
         {
            title = "Access materials ";
            suffix = ", or {{ if ($CFG/sopc_help_source == 'local') 'Online'; else 'Locally'; }} when not available.";
            key = "D";
            DATA
            {
               $FILE/sopc_help_source = "$";
               $CFG/sopc_help_source = "$";
            }
            ITEM
            {
               title = "Online";
               value = "web";
            }
            ITEM
            {
               title = "Locally";
               value = "local";
            }
         }
     }
     GROUP
     {
         title = "Web Browser";
        TEXT
        {
           title = "Application Path (Leave blank for system default)";
        }
        EDIT
        {
           DATA
           {
              sopc_browser_command = "$";
           }
           #TBD: tooltip="What this is used for";
           BUTTON
           {
              title = "...";
              ACTION select
              {
                 $FILE/sopc_browser_command = "{{ browse($FILE/sopc_browser_command, ''); }}";
              }
           }
        }
        GROUP { width = 1; height = 4; }
        GROUP
        {
           layout = "horizontal";
           align = "left";
           spacing = 4;
           CHECK proxy_set
           {
              title = "Use Proxy Host";
              DATA
              {
                 sopc_proxy_set = "$";
              }
              glue = 0;
           }
           EDIT proxy_host
           {
              enable = "{{ $CFG/sopc_proxy_set == 1 }}";
              columns = 14;
              glue = 0;
              DATA
              {
                 sopc_proxy_host = "$";
              }
           }
           EDIT proxy_port
           {
              title = " : ";
              enable = "{{ $CFG/sopc_proxy_set == 1 }}";
              columns = 4;
              DATA
              {
                 sopc_proxy_port = "$";
              }
           }
           GROUP { width = 100; height = 2; }
        }
      }
      GROUP sim
      {
         title = "HDL Simulator";
        spacing = "8";
        COMBO
        {
           suffix = " Application Path: ";
        	DATA { sopc_simulator_type = "$"; }
			ITEM
			{
				title = "ModelSim by ModelTech, Inc.";
				value = "modelsim";
			}
        }
        EDIT
        {
           type = "path";
           DATA
           {
              sopc_simulator_dir = "$";
           }
           error = "{{ if (regexp($,'.* .*')) 'Path to HDL Simulator cannot contain spaces'; }}";
           # SPR 174499: warn if vsim not found
           warning = "{{ if (($CFG/sopc_simulator_type == 'modelsim') && ($ != '' && !exists($+'/vsim'+$$exe_suffix))) 'Cannot locate '+$+'/vsim'+$$exe_suffix; }}";
           BUTTON
           {
              title = "...";
              ACTION select
              {
                 $FILE/sopc_simulator_dir = "{{ browse($FILE/sopc_simulator_dir, 'directory'); }}";
              }
           }
        }
	# 5.0 fixup for code which is dependent on sopc_modelsim_dir
	modelsim_fixup = "{{ if ($CFG/sopc_simulator_type == 'modelsim') { $CFG/sopc_modelsim_dir = $CFG/sopc_simulator_dir; } }}";
      }
      GROUP
      {
         title = "Component/Kit Library Search Path";
         REM
         {
            title = "Additional directories containing SOPC Builder components / kits";
         }
         EDIT
         {
            tooltip = 
            "List directories containing SOPC Builder components, separated<br>
             by plusses ('+'). Default is to search the current project directory, <br>
             then the built-in SOPC Builder component directories.  Directories listed<br>
             here will be searched first, in the order specified (left-to-right).
		{{ if ($$os == 'win32') '<br>NOTE: Forward slashes are used in place of backslashes.' }}";
            DATA
            {
               sopc_component_path = "$";
            }
            rows=3;
            type = "path";
            warning = "{{ if ($$save_sopc_path != $) 'Changes to search path will not take effect until SOPC Builder is restarted.' }}";
         }
      }
    GROUP messages
    {
        LIST
        {
            rows=3;
            glue=0;
            background="lightGray";
        }
    }
    GROUP dialog
    {
        BUTTON ok     { title="OK";      key="O"; action="dialog_ok"; default="1";     }
    }
   }

   WIZARD_UI advertisement
   {
      type = "dialog";
      title = "{{ if ($AD/window_title != '') $AD/window_title; else 'Available SOPC Builder Ready Core'; }}";
      spacing=6;
      align = "center";
      # logo-image which typically links to provider info page
      IMAGE logo
      {
         file="{{ $AD/logo_image }}";
         url="{{ $AD/logo_url }}";
         tooltip="{{ $AD/logo_tooltip }}";
      }
      # title text, large and centered beneath logo
      TEXT
      {
         title="{{ $AD/title }}";
         font="bold";
      }
      # a brief but scintillating tale revealing this core's lineage and its future in your system design(s)
      GROUP
      {
          layout="horizontal";
          GROUP { width = 20; height = 20; }
          TEXT
          {
              title="A no-cost OpenCore evaluation of the {{ $AD/name }} {{ if ($AD/provider != '') 'by '; }}{{ $AD/provider }} is available upon request.<br>
              This peripheral meets Altera's requirements for seamless integration within an {{ $AD/bus }}-based system, and is<br>
              designated by Altera as SOPC Builder Ready.  For a product overview, or to {{ $AD/verbiage }},<br>
              please click on the '{{ $AD/action }}' button below.";
          }
          GROUP { width = 20; height = 20; }
      }
      # remove the message-pane
      GROUP messages
      {
      }
      # special dialog buttons to take action or cancel
      GROUP dialog
      {
         GROUP buttons
         {
            layout = "horizontal";
            spacing = 15;
            BUTTON
            {
               title="{{ $AD/action }}"; 
               action="dialog_ok";
            }
            BUTTON
            {
               title="Cancel"; 
               action="dialog_cancel";
            }
         }
         GROUP { width=8; height=8; }
      }
   }

   # display upgrade warning message
   WIZARD_UI legacy_sdk_warning
   {
      title = "Legacy SDK Warning";
      align = "left";
      TEXT
      {
         title = "Legacy SDK is not supported for Nios II 6.0 and beyond.<br>Please migrate your software to use the HAL System Library.<br><br>Legacy SDK support has been removed from the PTF file.<br>Original PTF file has been saved with .bak extension in the project directory.";
      }

      # override standard OK/cancel with:
      GROUP dialog
      {
         BUTTON
         {
            title = "OK";
            action = "dialog_ok";
            default = 1;
         }
         GROUP
         {
            width = "8";
            height = "8";
         }
      }
   }
